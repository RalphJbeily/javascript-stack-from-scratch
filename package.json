  "scripts": {
    "start": "yarn dev:start",
    "dev:start":
      "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build":
      "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p",
    "prod:start": "heroku local",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test":
      "yarn lint && jest --coverage && codecov --token=82fb4784-9108-4ccc-bbd5-32ee7780d6fa",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build",
    "heroku-postbuild": "yarn prod:build"
  },
  "engines": {
    "node": "8.6.0",
    "yarn": "1.1.0"
  },
  "browserslist": ["> 1%"],
